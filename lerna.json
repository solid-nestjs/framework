{
  "version": "0.2.5",
  "npmClient": "npm",
  "packages": ["packages-core/*", "packages-bundles/*", "apps-examples/*"],
  "command": {
    "publish": {
      "conventionalCommits": true,
      "message": "chore(release): publish",
      "registry": "https://registry.npmjs.org/"
    },
    "version": {
      "allowBranch": ["main", "master"],
      "conventionalCommits": true,
      "message": "chore(release): %s"
    }
  }
}
