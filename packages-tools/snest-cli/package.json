{
  "name": "@solid-nestjs/snest-cli",
  "version": "0.3.0-alpha.1",
  "description": "CLI tool for generating SOLID NestJS applications and components",
  "keywords": [
    "nestjs",
    "cli",
    "generator",
    "solid",
    "typescript",
    "crud",
    "graphql",
    "rest"
  ],
  "author": "SOLID NestJS Team",
  "license": "MIT",
  "bin": {
    "snest": "./bin/snest.js"
  },
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "files": [
    "dist",
    "bin",
    "templates"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.12",
    "ora": "^6.3.1",
    "prettier": "^3.2.5",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/validate-npm-package-name": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solid-nestjs/framework.git",
    "directory": "packages-tools/snest-cli"
  },
  "bugs": {
    "url": "https://github.com/solid-nestjs/framework/issues"
  },
  "homepage": "https://github.com/solid-nestjs/framework/tree/main/packages-tools/snest-cli#readme"
}
